{
  "description": "HTML rendering configuration for ePy_docs - Reino HTML espec√≠fico",
  "version": "1.0.0",
  
  "quarto_html_config": {
    "theme": "default",
    "toc": true,
    "toc-depth": 3,
    "number-sections": true,
    "highlight-style": "github",
    "code-fold": false,
    "code-tools": true
  },
  
  "rendering": {
    "engine": "quarto",
    "output_format": "html",
    "css_injection": true,
    "math_renderer": "katex"
  },
  
  "optimization": {
    "minify_css": false,
    "minify_js": false,
    "embed_resources": false
  },
  
  "html_specific": {
    "responsive": true,
    "accessibility": true,
    "semantic_markup": true,
    "metadata": {
      "generator": "ePy_docs + Quarto"
    }
  }
}
