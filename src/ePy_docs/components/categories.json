{
  "category_rules": {
    "nodes": {
      "name_keywords": ["coordinate", "position", "coord", "node", "point", "location"],
      "coordinate_patterns": ["x", "y", "z", "r", "theta", "phi"],
      "description": "Geometric coordinates and nodal positions"
    },
    "support": {
      "name_keywords": ["support", "constraint", "boundary", "restraint", "reaction", "bearing", "support type"],
      "value_keywords": ["pinned", "fixed", "free", "none", "foundation", "restrained", "roller", "hinge"],
      "description": "Structural support conditions and constraints"
    },
    "status": {
      "name_keywords": ["status", "state", "condition", "check", "verification", "result"],
      "value_keywords": ["pass", "fail", "warning", "pending", "active", "inactive", "ok", "error", "critical"],
      "description": "Analysis and verification status indicators"
    },
    "elements": {
      "name_keywords": ["element", "member", "component", "section", "element type"],
      "value_keywords": ["beam", "column", "brace", "foundation", "slab", "wall", "truss", "frame", "plate"],
      "description": "Structural element types and components"
    },
    "material": {
      "name_keywords": ["material", "substance", "grade", "class", "spec"],
      "value_keywords": ["steel", "concrete", "wood", "composite", "aluminum", "masonry", "reinforced"],
      "description": "Material properties and specifications"
    },
    "forces": {
      "name_keywords": ["force", "load", "moment", "shear", "axial", "torsion"],
      "value_keywords": ["tension", "compression", "bending", "distributed", "point", "uniform"],
      "description": "Forces, loads, and internal stresses"
    },
    "dimensions": {
      "name_keywords": ["length", "width", "height", "depth", "diameter", "thickness", "span"],
      "coordinate_patterns": ["mm", "m", "cm", "in", "ft"],
      "description": "Physical dimensions and measurements"
    },
    "properties": {
      "name_keywords": ["area", "inertia", "modulus", "strength", "capacity", "resistance"],
      "value_keywords": ["elastic", "plastic", "ultimate", "yield", "design", "nominal"],
      "description": "Mechanical and geometric properties"
    },
    "design": {
      "name_keywords": ["design", "capacity", "demand", "utilization", "ratio", "factor"],
      "value_keywords": ["safe", "unsafe", "adequate", "inadequate", "critical", "acceptable"],
      "description": "Design parameters and safety factors"
    },
    "analysis": {
      "name_keywords": ["analysis", "calculation", "result", "output", "response"],
      "value_keywords": ["linear", "nonlinear", "dynamic", "static", "buckling", "vibration"],
      "description": "Analysis types and computational results"
    }
  },
  "coloring_schemes": {
    "status_based": {
      "pass": "#4CAF50",
      "fail": "#F44336", 
      "warning": "#FF9800",
      "pending": "#9E9E9E",
      "critical": "#D32F2F",
      "ok": "#8BC34A"
    },
    "intensity_based": {
      "low": "#E8F5E8",
      "medium": "#FFF3E0",
      "high": "#FFEBEE",
      "critical": "#F44336"
    },
    "element_based": {
      "beam": "#2196F3",
      "column": "#FF9800", 
      "foundation": "#795548",
      "slab": "#9C27B0",
      "brace": "#607D8B"
    }
  },
  "format_rules": {
    "coordinates": {
      "precision": 3,
      "units": "m",
      "format": "decimal"
    },
    "forces": {
      "precision": 2,
      "units": "kN",
      "format": "scientific_if_large"
    },
    "stresses": {
      "precision": 1,
      "units": "MPa", 
      "format": "decimal"
    },
    "ratios": {
      "precision": 3,
      "units": "",
      "format": "percentage"
    }
  }
}
