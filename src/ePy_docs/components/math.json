{
  "mathematical_processing": {
    "supported_formats": ["latex", "mathml", "ascii"],
    "default_renderer": "latex",
    "equation_numbering": true,
    "inline_math_delimiters": ["$", "$"],
    "display_math_delimiters": ["$$", "$$"],
    "processing_settings": {
      "auto_convert": true,
      "preserve_spacing": true,
      "error_handling": "strict"
    }
  }
}
