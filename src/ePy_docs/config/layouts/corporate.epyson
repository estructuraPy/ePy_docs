  "code_chunks": {
    "spacing": {
      "before": "\n\n",
      "after": "\n\n"
    },
    "caption_format": "**{caption}**"
  },
  "figures": {
    "defaults": {
      "alt_text": "Figure",
      "width": "6.5in"
    },
    "plot_settings": {
      "bbox_inches": "tight",
      "dpi": 300,
      "facecolor": "layout_background",
      "format": "png"
    },
    "caption": {
      "numbering": false,
      "style": "bold",
      "format": "**{caption}**",
      "reference_format": "@fig-{id}"
    },
    "caption_position": "bottom",
    "caption_format": "**{caption}**",
    "spacing": {
      "before": "\n\n",
      "after": "\n\n"
    },
    "alignment": "left",
    "default_width": "6.5in",
    "include_alt_text": false
  },
  "tables": {
    "alignment": "left",
    "alternating_rows": false,
    "caption_format": ": **{caption}**",
    "caption_position": "top",
    "header_bold": true,
    "spacing": {
      "before": "\n\n",
      "after": "\n\n"
    },
    "styling": {
      "alternating_rows": false,
      "cell_padding": 0.03,
      "content_bold": false,
      "grid_width_px": 0.25,
      "header_bold": true,
      "header_wrap_max_chars": 35,
      "highlight_node_column": true,
      "line_spacing": 1.3,
      "right_padding_spaces": 3,
      "width_in": 7.0
    },
    "defaults": {
      "alignment": {
        "content": {
          "horizontal": "right",
          "vertical": "center"
        },
        "header": {
          "horizontal": "center",
          "vertical": "center"
        },
        "numeric": {
          "horizontal": "right"
        },
        "title": {
          "horizontal": "center"
        }
      },
      "borders": {
        "style": "solid",
        "width_px": 0.25
      },
      "category_detection": {
        "analysis": [
          "analysis"
        ],
        "design": [
          "design"
        ],
        "dimensions": [
          "dimensions"
        ],
        "forces": [
          "forces"
        ],
        "general": [
          "general"
        ],
        "nodes": [
          "nodes"
        ],
        "properties": [
          "properties"
        ]
      },
      "transparency": {
        "alt_row_alpha": 0.2,
        "border_alpha": 0.7,
        "grid_alpha": 0.5,
        "header_alpha": 0.9
      }
    }
