  "engineering_notation": {
    "description": "Engineering notation prefixes (powers of 1000)",
    "preferred_powers": [0, 3, 6, 9, 12, 15, 18, -3, -6, -9, -12, -15, -18],
    "common_engineering": {
      "10^3": "k",
      "10^6": "M",
      "10^9": "G",
      "10^12": "T",
      "10^-3": "m",
      "10^-6": "μ",
      "10^-9": "n",
      "10^-12": "p"
    }
