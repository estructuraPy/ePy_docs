  "parsing_rules": {
    "case_sensitivity": {
      "strict": ["M", "m", "K", "k", "G", "g"],
      "note": "Capital M = mega, lowercase m = milli"
    },
    "spacing": {
      "no_space": "standard",
      "examples": ["km", "MPa", "kN⋅m"]
    },
    "recognition_patterns": {
      "prefix_unit": "^([a-zA-Z]{1,2})([a-zA-Z]+)$",
      "compound_unit": "^([a-zA-Z]+)[⋅*/^-]([a-zA-Z]+)$"
    }
  }
