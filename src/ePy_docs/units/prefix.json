  "usage_rules": {
    "standard_units": {
      "length": ["mm", "cm", "m", "km"],
      "mass": ["mg", "g", "kg", "t"],
      "time": ["ms", "s", "min", "h"],
      "force": ["mN", "N", "kN", "MN"],
      "pressure": ["Pa", "kPa", "MPa", "GPa"],
      "energy": ["mJ", "J", "kJ", "MJ", "GJ"],
      "power": ["mW", "W", "kW", "MW", "GW"]
    },
    "preferred_engineering": {
      "structural": ["mm", "kN", "MPa", "kN⋅m"],
      "civil": ["m", "kN", "kPa", "MN"],
      "mechanical": ["mm", "N", "Pa", "J"],
      "electrical": ["mA", "V", "kΩ", "mW"]
    },
    "avoid_combinations": [
      "hecto", "deca", "deci",
      "rarely used in engineering"
    ]
  },
