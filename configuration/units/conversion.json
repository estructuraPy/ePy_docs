  "compound_units": {
    "description": "Rules for handling compound units with multiple dimensions",
    "parsing_rules": {
      "multiplication": ["⋅", "*", "-", " "],
      "division": ["/", " per "],
      "exponentiation": ["^", "²", "³", "⁴", "⁵"]
    },
    "operator_precedence": {
      "exponentiation": 3,
      "multiplication": 2,
      "division": 1
    },
    "examples": {
      "area": {
        "units": ["m^2", "ft^2", "cm^2", "mm^2", "in^2"],
        "conversion_rule": "square the linear conversion factor"
      },
      "volume": {
        "units": ["m^3", "ft^3", "cm^3", "mm^3", "in^3", "L"],
        "conversion_rule": "cube the linear conversion factor"
      },
      "velocity": {
        "units": ["m/s", "ft/s", "km/h", "mph"],
        "conversion_rule": "divide length by time conversion factors"
      },
      "acceleration": {
        "units": ["m/s^2", "ft/s^2", "g"],
        "conversion_rule": "divide length by time squared conversion factors"
      },
      "force_per_length": {
        "units": ["N/m", "lbf/ft", "kN/m", "kip/ft"],
        "conversion_rule": "divide force by length conversion factors"
      },
      "moment": {
        "units": ["N⋅m", "lbf⋅ft", "kN⋅m", "kip⋅ft"],
        "conversion_rule": "multiply force by length conversion factors"
      },
      "stress": {
        "units": ["Pa", "psi", "MPa", "ksi", "kgf/cm^2"],
        "conversion_rule": "divide force by area conversion factors"
      }
    }
