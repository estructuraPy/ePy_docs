    "dimensional_consistency": true,
    "unit_recognition": {
      "case_sensitive": false,
      "allow_abbreviations": true,
      "allow_unicode": true
    },
    "error_handling": {
      "unknown_unit": "warn",
      "dimension_mismatch": "error",
      "overflow": "error"
    }
