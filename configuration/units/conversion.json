  "description": "Comprehensive conversion factors and formulas for engineering units",
  "version": "2.1",
  "last_updated": "2025-07-12",
  "systems": ["SI", "Imperial", "Mixed", "Engineering"],
  "unit_delimiters": {
    "parentheses": ["(", ")"],
    "brackets": ["[", "]"],
    "braces": ["{", "}"],
    "angle_brackets": ["<", ">"],
    "pipes": ["|", "|"],
    "slashes": ["/", "/"]
  },
  "conversion_methods": {
    "linear": {
      "description": "Linear conversion: target = source * factor + offset",
      "formula": "target = source * factor + offset",
      "default_offset": 0
    },
    "temperature": {
      "description": "Temperature conversions with different formulas",
      "celsius_to_fahrenheit": "F = C * 9/5 + 32",
      "fahrenheit_to_celsius": "C = (F - 32) * 5/9",
      "celsius_to_kelvin": "K = C + 273.15",
      "kelvin_to_celsius": "C = K - 273.15",
      "fahrenheit_to_rankine": "R = F + 459.67",
      "rankine_to_fahrenheit": "F = R - 459.67"
    },
    "compound": {
      "description": "Compound units require multiplication of individual conversion factors",
      "examples": ["m^2", "ft/s^2", "kN⋅m", "lbf/in^2"]
    }
  },
